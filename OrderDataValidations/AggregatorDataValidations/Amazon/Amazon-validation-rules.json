{
   "schema": {
      "aggregator_name": {
         "type": "string",
         "allowed": [
            "AMAZON"
         ]
      },
      "price_type": {
         "type": "string",
         "allowed": [
            "Retail Price"
         ]
      },
      "sale_type_ori": {
         "type": "string",
         "regex": "[a-zA-Z]*"
      },
      "sale_type": {
         "type": "string",
         "regex": "[a-zA-Z]*",
         "allowed": [
            "Purchase",
            "rental_type",
            "Checkout",
            "Extension"
         ]
      },
      "source": {
         "type": "string",
         "allowed": [
            "Amazon",
            "Amazon Rental"
         ]
      },
      "reporting_date": {
         "type": "date",
         "regex": "[0-9]{2}/[0-9]{2}/[0-9]{4}"
      },
      "external_product_id": {
         "type": "string",
         "regex": "[A-Z0-9]*"
      },
      "external_product_id_type": {
         "type": "string",
         "allowed": [
            "ASIN"
         ]
      },
      "product_type": {
         "type": "string",
         "allowed": [
            "Electronic"
         ]
      }
   }
}