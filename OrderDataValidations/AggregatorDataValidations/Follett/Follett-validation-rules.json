{
   "schema": {
      "aggregator_name": {
         "type": "string",
         "allowed": [
            "FOLLETT"
         ]
      },
      "price_currency": {
         "type": "string",
         "allowed": [
            "USD"
         ]
      },
      "payment_amount_currency": {
         "type": "string",
         "allowed": [
            "USD"
         ]
      },
      "trans_type": {
         "type": "string",
         "regex": "[a-zA-Z]*",
         "allowed": [
            "Sales"
         ]
      },
      "sale_type_ori": {
         "type": "string",
         "regex": "[a-zA-Z]*"
      },
      "sale_type": {
         "type": "string",
         "regex": "[a-zA-Z]*",
         "allowed": [
            "Purchase",
            "Return"
         ]
      },
      "source": {
         "type": "string",
         "allowed": [
            "FOLLETT"
         ]
      },
      "reporting_date": {
         "type": "string",
         "regex": "[0-9]{2}/[0-9]{2}/[0-9]{4}"
      },
      "product_type": {
         "type": "string",
         "allowed": [
            "Electronic"
         ]
      }
   }
}