{
   "schema": {
      "aggregator_name": {
         "type": "string"
      },
      "trans_type": {
         "type": "string",
         "regex": "[a-zA-Z]*",
         "allowed": [
           "Sales",
           "Gratis"
         ]
      },
      "price_currency": {
         "type": "string",
         "allowed": [
            "AUD"
         ]
      },
      "publisher_price_ori_currency": {
         "type": "string",
         "allowed": [
            "AUD"
         ]
      },
      "payment_amount_currency": {
         "type": "string",
         "allowed": [
            "AUD",
            "NZD"
         ]
      },
      "sale_type": {
         "type": "string",
         "regex": "[a-zA-Z]*",
         "allowed": [
            "Purchase",
            "Return"
         ]
      },
      "ex_currencies": {
         "type": "string",
         "allowed": [
            "AUD/NZD",
            "NA"
         ]
      },
      "product_type": {
         "type": "string",
         "allowed": [
            "Print"
         ]
      }
   }
}