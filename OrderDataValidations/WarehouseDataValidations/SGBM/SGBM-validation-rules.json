{
   "schema": {
      "aggregator_name": {
         "type": "string"
      },
      "trans_type": {
         "type": "string",
         "regex": "[a-zA-Z]*",
         "allowed": [
           "Sales",
           "Gratis"
         ]
      },
      "price_currency": {
         "type": "string",
         "allowed": [
            "SGD"
         ]
      },
      "publisher_price_ori_currency": {
         "type": "string",
         "allowed": [
            "GBP",
            "NA"
         ]
      },
      "payment_amount_currency": {
         "type": "string",
         "allowed": [
            "SGD"
         ]
      },
      "sale_type": {
         "type": "string",
         "regex": "[a-zA-Z]*",
         "allowed": [
            "Purchase",
            "Return"
         ]
      },
      "ex_currencies": {
         "type": "string",
         "allowed": [
            "SGD/USD",
            "SGD/GBP",
            "NA"
         ]
      },
      "product_type": {
         "type": "string",
         "allowed": [
            "Print"
         ]
      }
   }
}