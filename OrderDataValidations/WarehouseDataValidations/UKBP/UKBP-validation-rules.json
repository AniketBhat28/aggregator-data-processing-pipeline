{
   "schema": {
      "aggregator_name": {
         "type": "string"
      },
      "trans_type": {
         "type": "string",
         "regex": "[a-zA-Z]*",
         "allowed": [
           "Sales",
           "Gratis",
           "Transfer",
           "Stock Movement"
         ]
      },
      "price_currency": {
         "type": "string",
         "allowed": [
            "GBP"
         ]
      },
      "publisher_price_ori_currency": {
         "type": "string",
         "allowed": [
            "GBP"
         ]
      },
      "payment_amount_currency": {
         "type": "string",
         "allowed": [
            "GBP"
         ]
      },
      "sale_type": {
         "type": "string",
         "regex": "[a-zA-Z]*",
         "allowed": [
            "Purchase",
            "Return"
         ]
      },
      "ex_currencies": {
         "type": "string",
         "allowed": [
            "GBP/EUR",
            "GBP/USD"
         ]
      },
      "sale_type_ori": {
         "type": "string",
         "allowed": [
            "I",
            "C"
         ]
      },
      "product_type": {
         "type": "string",
         "allowed": [
            "Print"
         ]
      }
   }
}