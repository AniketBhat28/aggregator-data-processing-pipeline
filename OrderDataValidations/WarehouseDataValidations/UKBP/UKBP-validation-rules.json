{
   "schema": {
      "aggregator_name": {
         "type": "string"
      },
      "trans_type": {
         "type": "string",
         "regex": "[a-zA-Z]*",
         "allowed": [
           "Sales",
           "Gratis",
           "Transfer",
           "Stock Movement"
         ]
      },
      "price_currency": {
         "type": "string",
         "allowed": [
            "GBP"
         ]
      },
      "publisher_price_ori_currency": {
         "type": "string",
         "nullable": true,
         "allowed": [
            "GBP"
         ]
      },
      "payment_amount_currency": {
         "type": "string",
         "allowed": [
            "GBP",
            "EUR",
            "USD"
         ]
      },
      "sale_type": {
         "type": "string",
         "nullable": true,
         "regex": "[a-zA-Z]*",
         "allowed": [
            "Purchase",
            "Return"
         ]
      },
      "ex_currencies": {
         "type": "string",
         "nullable": true,
         "allowed": [
            "GBP/EUR",
            "GBP/USD",
            "NA"
         ]
      },
      "sale_type_ori": {
         "type": "string",
         "allowed": [
            "I",
            "C"
         ]
      },
      "product_type": {
         "type": "string",
         "allowed": [
            "Print"
         ]
      },
      "pod_ori": {
         "type": "string",
         "allowed": [
            "CPDT","CPTT","CRAR","CRCA","CRCN","CRCR","CRGC","CRIN","CRIS","CRNB","CRNE","CRNO","CRSG","CRSW","CRUG","CRUP","CRUR","CRXE","CRXN","CRXT","CXNS",
            "EDRF","EDRS","EDTF","EDTS",
            "GRXN","GXNS",
            "IASP","IASS","IGRX","INCA","INCH","INCN","INCP","INCR","INGC","INGR","INSO","INST","INV","INXN","IPDT","IPGT","IPTT","IRIS","IRSW","IXNS",
            "WRCN","WRCS","WRNB","WRUG"
         ]
      },
      "sa_pack_flag": {
         "type": "string",
         "nullable": true,
         "allowed": [
            "P",
            "C"
         ]
      }
   }
}